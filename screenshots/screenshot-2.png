<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="800" height="600" fill="#f5f5f5"/>
  
  <!-- Browser chrome -->
  <rect width="800" height="40" fill="#e0e0e0"/>
  <circle cx="20" cy="20" r="6" fill="#ff5f56"/>
  <circle cx="40" cy="20" r="6" fill="#ffbd2e"/>
  <circle cx="60" cy="20" r="6" fill="#27c93f"/>
  
  <!-- Content area -->
  <rect x="50" y="70" width="700" height="500" fill="#ffffff" stroke="#ddd" stroke-width="1" rx="8"/>
  
  <!-- Header -->
  <text x="400" y="110" text-anchor="middle" font-family="Arial, sans-serif" font-size="24" font-weight="bold" fill="#23282d">
    Two-Factor Authentication Settings
  </text>
  
  <!-- Setup Authenticator App section -->
  <rect x="70" y="140" width="660" height="400" fill="#f9f9f9" rx="6"/>
  <text x="85" y="165" font-family="Arial, sans-serif" font-size="18" font-weight="bold" fill="#23282d">
    Setup Authenticator App
  </text>
  
  <!-- Steps -->
  <text x="100" y="200" font-family="Arial, sans-serif" font-size="13" fill="#666">
    1. Download an authenticator app (Google Authenticator, Authy, etc.)
  </text>
  <text x="100" y="225" font-family="Arial, sans-serif" font-size="13" fill="#666">
    2. Scan the QR code below with your app
  </text>
  <text x="100" y="250" font-family="Arial, sans-serif" font-size="13" fill="#666">
    3. Enter the code from your app to verify
  </text>
  
  <!-- QR Code -->
  <rect x="300" y="270" width="200" height="200" fill="#ffffff" stroke="#ddd" stroke-width="1" rx="4"/>
  
  <!-- QR Code pattern -->
  <rect x="320" y="290" width="40" height="40" fill="#000"/>
  <rect x="370" y="290" width="10" height="10" fill="#000"/>
  <rect x="390" y="290" width="10" height="10" fill="#000"/>
  <rect x="410" y="290" width="10" height="10" fill="#000"/>
  <rect x="440" y="290" width="40" height="40" fill="#000"/>
  
  <rect x="320" y="340" width="10" height="10" fill="#000"/>
  <rect x="350" y="340" width="10" height="10" fill="#000"/>
  <rect x="390" y="340" width="20" height="20" fill="#000"/>
  <rect x="440" y="340" width="10" height="10" fill="#000"/>
  <rect x="470" y="340" width="10" height="10" fill="#000"/>
  
  <rect x="330" y="350" width="20" height="20" fill="#fff"/>
  <rect x="450" y="350" width="20" height="20" fill="#fff"/>
  
  <rect x="370" y="360" width="10" height="10" fill="#000"/>
  <rect x="410" y="360" width="10" height="10" fill="#000"/>
  
  <rect x="390" y="380" width="20" height="20" fill="#000"/>
  
  <rect x="320" y="410" width="40" height="40" fill="#000"/>
  <rect x="370" y="410" width="10" height="10" fill="#000"/>
  <rect x="410" y="410" width="10" height="10" fill="#000"/>
  <rect x="440" y="410" width="10" height="10" fill="#000"/>
  <rect x="470" y="410" width="10" height="10" fill="#000"/>
  
  <rect x="330" y="420" width="20" height="20" fill="#fff"/>
  
  <!-- Secret key -->
  <text x="400" y="490" text-anchor="middle" font-family="Arial, sans-serif" font-size="13" fill="#666">
    Secret Key:
  </text>
  <rect x="315" y="495" width="170" height="24" fill="#f0f0f0" rx="3"/>
  <text x="400" y="512" text-anchor="middle" font-family="Courier New, monospace" font-size="13" fill="#23282d">
    ABCD-EFGH-IJKL-MNOP
  </text>
  
  <!-- Verification code input -->
  <text x="200" y="560" font-family="Arial, sans-serif" font-size="13" font-weight="bold" fill="#23282d">
    Verification Code:
  </text>
  <rect x="330" y="545" width="100" height="30" fill="#ffffff" stroke="#ddd" stroke-width="1" rx="4"/>
  <text x="380" y="567" text-anchor="middle" font-family="Courier New, monospace" font-size="16" fill="#999">
    000000
  </text>
  <rect x="440" y="545" width="80" height="30" fill="#0073aa" rx="4"/>
  <text x="480" y="567" text-anchor="middle" font-family="Arial, sans-serif" font-size="13" font-weight="bold" fill="#fff">
    Verify
  </text>
</svg>
